{
  "manifest_version": 3,
  "name": "TabNum",
  "version": "1.0.0",
  "description": "Tabの位置を教えてくれます",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "toriringo.png",
      "48": "toriringo.png",
      "128": "toriringo.png"
    }
  }
}
